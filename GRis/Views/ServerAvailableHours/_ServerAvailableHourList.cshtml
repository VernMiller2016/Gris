@using GRis.Core.Extensions
@model IEnumerable<GRis.ViewModels.ServerAvailableHourModels.ServerAvailableHourDetailsViewModel>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ServerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServerVendorId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateRange)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AvailableHours)
        </th>
        @*<th></th>*@
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ServerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServerVendorId)
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.DateRange)*@
            @item.DateRange.ToMonthYear()
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AvailableHours)
        </td>
        @*<td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>*@
    </tr>
}

</table>
